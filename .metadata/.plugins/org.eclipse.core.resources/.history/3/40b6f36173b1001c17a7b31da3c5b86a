package ezmet.enver.calcualter;

import com.godtsoft.diyjava.DIYWindow;

public class Calculater_2 extends DIYWindow {

	public Calculater_2() {
		setFontSize(25);
		print("Please Enter A number");
		String s=input();
		int a=Integer.parseInt(s);
		print("a is"+a);
		print("Enter another number");
		s=input();
		int b=Integer.parseInt(s);
		print("b is "+b);
		int c=a+b;
		print(a + "+"+b+"="+c);
		
		
		
	}
	
	private int promtForInt(String promt){
		
		int i=0;
		print(promt);
		String s=input();
		i=Integer.parseInt(s);
		return i;
		
		
	}

	public static void main(String[] args) {
		new Calculater_2();

	}

}
